      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug website agent",
      "type": "debugpy",
      "request": "launch",
      "module": "agents.website.main",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug marketing agent",
      "type": "debugpy",
      "request": "launch",
